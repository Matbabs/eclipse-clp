{
    "name": "eclipse-clp_syntax-highlight",
    "publisher": "matbabs",
    "icon": "assets/eclipse.png",
    "displayName": "eclipse-clp_syntax-highlight",
    "description": "syntax highlight for \"ecl\" files.",
    "version": "0.0.4",
    "repository": "https://github.com/Matbabs/eclipse-clp",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ecl",
            "aliases": ["eclipse-clp", "ecl"],
            "extensions": [".ecl", ".pl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ecl",
            "scopeName": "source.ecl",
            "path": "./syntaxes/ecl.tmLanguage.json"
        }]
    }
}